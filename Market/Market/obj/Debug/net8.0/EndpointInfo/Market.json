  "components": {
    "schemas": {
      "AnnouncementDto": {
        "required": [
          "category",
          "contactPreference",
          "description",
          "phoneNumber",
          "price",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "maxLength": 2000,
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "isNegotiable": {
            "type": "boolean"
          },
          "category": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "phoneNumber": {
            "maxLength": 15,
            "minLength": 1,
            "type": "string"
          },
          "contactPreference": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "vehicleData": {
            "$ref": "#/components/schemas/VehicleData"
          },
          "partData": {
            "$ref": "#/components/schemas/PartData"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordDto": {
        "required": [
          "currentPassword",
          "newPassword"
        ],
        "type": "object",
        "properties": {
          "currentPassword": {
            "minLength": 8,
            "type": "string"
          },
          "newPassword": {
            "minLength": 8,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PartData": {
        "required": [
          "compatibility",
          "condition",
          "partName"
        ],
        "type": "object",
        "properties": {
          "partName": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "partNumber": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          },
          "condition": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "compatibility": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "engineVersion": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserDto": {
        "required": [
          "email",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "name": {
            "maxLength": 100,
            "type": "string",
            "nullable": true
          },
          "surname": {
            "maxLength": 100,
            "type": "string",
            "nullable": true
          },
          "phoneNumber": {
            "maxLength": 15,
            "type": "string",
            "nullable": true
          },
          "hasCompletedProfilePrompt": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "VehicleData": {
        "required": [
          "bodyType",
          "brand",
          "color",
          "condition",
          "drive",
          "engine",
          "equipment",
          "formalities",
          "fuelType",
          "history",
          "mileage",
          "model",
          "transmission",
          "year"
        ],
        "type": "object",
        "properties": {
          "brand": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "model": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "year": {
            "maximum": 2100,
            "minimum": 1900,
            "type": "integer",
            "format": "int32"
          },
          "equipmentVersion": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          },
          "bodyType": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "color": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "engine": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "fuelType": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "transmission": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "drive": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "mileage": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "condition": {
            "maxLength": 2000,
            "minLength": 1,
            "type": "string"
          },
          "history": {
            "maxLength": 2000,
            "minLength": 1,
            "type": "string"
          },
          "equipment": {
            "maxLength": 1000,
            "minLength": 1,
            "type": "string"
          },
          "formalities": {
            "maxLength": 1000,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Please enter JWT with Bearer into field (e.g., 'Bearer your_token_here')",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}